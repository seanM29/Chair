<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_setting_manu"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView

        android:id="@+id/textView9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"

        android:textSize="17sp"
        android:textColor="#000000"
        android:textStyle="bold"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />
    <TextView
        android:layout_marginTop="20dp"
        android:id="@+id/textView19"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="BACK"
        android:textSize="17sp"
        android:textColor="#000000"
        android:textStyle="bold"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <com.seanshend.view.chairview.ChairView
        android:id="@+id/back2"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_below="@id/textView19" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/back2"
        android:gravity="center_horizontal"
        android:text="50"
        android:textColor="#ff4c9fff"
        android:textSize="10sp" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView2"
        android:orientation="horizontal">

        <Button
            android:id="@+id/backMinusBtn"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:background="@drawable/minus" />

        <com.seanshend.view.lineview.lineview
            android:id="@+id/line1"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            />

        <Button
            android:id="@+id/backAddBtn"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:background="@drawable/add" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout2"
        android:textColor="#000000"
        android:gravity="center_horizontal"
        android:text="SEAT"
        android:textSize="17sp"
        android:textStyle="bold" />

    <com.seanshend.view.chairview.ChairView
        android:id="@+id/seat2"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_below="@id/textView10" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/seat2"
        android:gravity="center_horizontal"
        android:text="30"
        android:textColor="#ff000000"
        android:textSize="10sp" />

    <LinearLayout
        android:id="@+id/seatline"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView3"
        android:orientation="horizontal">

        <Button
            android:id="@+id/seatMinusBtn"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:background="@drawable/minus" />

        <com.seanshend.view.lineview.lineview
            android:id="@+id/line2"
            android:layout_height="20dp"
            android:layout_width="20dp"
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            />

        <Button
            android:id="@+id/seatAddBtn"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:background="@drawable/add" />
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_marginTop="13dp"
        android:layout_below="@+id/seatline"
        android:layout_alignParentStart="true">

        <ProgressBar
            android:id="@+id/progressBar"
            style="@android:style/Widget.ProgressBar.Horizontal"
            android:layout_width="fill_parent"
            android:layout_height="35dp"
            android:layout_marginTop="14dp"
            android:max="90"
            android:progress="90"
            android:progressDrawable="@drawable/progressbar"
            android:visibility="visible" />

        <TextView
            android:id="@+id/tv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/progressBar"
            android:layout_alignTop="@+id/progressBar"
            android:layout_centerInParent="true"
            android:paddingTop="5dp"
            android:text="START"

            android:textColor="#ffffff"
            android:textSize="20sp" />
    </RelativeLayout>


</RelativeLayout>
