<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_data_week"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
   android:paddingTop="6dp"
    tools:context="com.seanshend.myapplication.Data_week">

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp">

        <Button
            android:id="@+id/button"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:layout_weight="1"
            android:background="#FFFFFF"
            android:onClick="GoDay"
            android:text="Day" />

        <Button
            android:id="@+id/button6"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:layout_weight="1"
            android:background="#E8E8E8"
            android:onClick="GoManu"
            android:text="Week" />
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/r1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout1">

        <com.seanshend.view.Circle.Circle
            android:id="@+id/c1"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="25dp" />

        <TextView
            android:id="@+id/t1"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="20dp"

            android:layout_toRightOf="@id/c1"
            android:text="Back Score"
            android:textSize="20sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/t2"
            android:layout_width="wrap_content"

            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/t1"
            android:layout_marginLeft="30dp"
            android:paddingBottom="30dp"
            android:text="2000"
            android:textSize="20sp" />

        <com.seanshend.view.dataview.weekview
            android:id="@+id/d1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/t2"

            android:layout_alignParentTop="true"
            android:layout_marginLeft="5dp"

            android:layout_toRightOf="@+id/t1" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/r2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/r1">

        <com.seanshend.view.Circle.Circle
            android:id="@+id/c2"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="25dp" />

        <TextView
            android:id="@+id/t3"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="20dp"

            android:layout_toRightOf="@id/c2"
            android:text="HIPS Score"
            android:textSize="20sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/t4"
            android:layout_width="wrap_content"

            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/t3"
            android:layout_marginLeft="30dp"
            android:paddingBottom="30dp"
            android:text="2000"
            android:textSize="20sp" />

        <com.seanshend.view.dataview.weekview
            android:id="@+id/d2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/t4"

            android:layout_alignParentTop="true"
            android:layout_marginLeft="5dp"

            android:layout_toRightOf="@+id/t3" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/r3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/r2">

        <com.seanshend.view.Circle.Circle
            android:id="@+id/c3"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="25dp" />

        <TextView
            android:id="@+id/t5"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="20dp"

            android:layout_toRightOf="@id/c3"
            android:text="WAIST Score"
            android:textSize="20sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/t6"
            android:layout_width="wrap_content"

            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/t5"
            android:layout_marginLeft="30dp"
            android:paddingBottom="30dp"
            android:text="2000"
            android:textSize="20sp" />

        <com.seanshend.view.dataview.weekview
            android:id="@+id/d3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/t6"

            android:layout_alignParentTop="true"
            android:layout_marginLeft="5dp"

            android:layout_toRightOf="@+id/t5" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/r4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/r3">

        <com.seanshend.view.Circle.Circle
            android:id="@+id/c4"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="25dp" />

        <TextView
            android:id="@+id/t7"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="20dp"

            android:layout_toRightOf="@id/c4"
            android:text="THIGH Score"
            android:textSize="20sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/t8"
            android:layout_width="wrap_content"

            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/t7"
            android:layout_marginLeft="30dp"
            android:paddingBottom="30dp"
            android:text="2000"
            android:textSize="20sp" />

        <com.seanshend.view.dataview.weekview
            android:id="@+id/d4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/t8"

            android:layout_alignParentTop="true"
            android:layout_marginLeft="5dp"

            android:layout_toRightOf="@+id/t7" />

    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="bottom"
        android:paddingBottom="8dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="15dp"
            android:layout_alignBaseline="@+id/r1"
            android:layout_alignParentStart="true">

            <ImageButton
                android:id="@+id/button2"
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"

                android:layout_marginLeft="35dp"
                android:layout_marginRight="50dp"
                android:onClick="GoHome"
                android:background="@drawable/home1" />

            <ImageButton
                android:id="@+id/button3"
                android:layout_width="30dp"

                android:layout_height="wrap_content"
                android:layout_marginRight="50dp"
                android:layout_toRightOf="@+id/button2"
                android:background="@drawable/data2"
                android:onClick="GoData"

                />

            <ImageButton
                android:id="@+id/button4"
                android:layout_width="30dp"

                android:layout_height="wrap_content"
                android:layout_marginRight="50dp"
                android:layout_toRightOf="@+id/button3"
                android:background="@drawable/setting1"
                android:onClick="GoSetting" />

            <ImageButton
                android:id="@+id/button5"
                android:layout_width="30dp"

                android:layout_height="wrap_content"
                android:layout_marginRight="40dp"
                android:layout_toRightOf="@+id/button4"
                android:background="@drawable/user1"
                android:onClick="GoProfile" />
        </RelativeLayout>
    </LinearLayout>
</RelativeLayout>
